{
  "name": "sl-grid",
  "version": "0.1.3",
  "description": "Simple angular2 grid based on directives",
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "test": "tsc && karma start",
    "postinstall": "typings install",
    "prepublish": "tsc",
    "tsc": "tsc",
    "typings": "typings",
    "transpile": "ngc",
    "package": "rollup -c",
    "build": "npm run transpile && npm run package && cp README.md dist/README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stratslab/sl-grid"
  },
  "author": {
    "name": "Stanisław Wierchoła",
    "email": "stanislaw.wierchola@stratslab.com"
  },
  "keywords": [
    "angular2"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stratslab/sl-grid/issues"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@angular/core": "^2.0.0",
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/compiler-cli": "^2.0.0",
    "codelyzer": "^0.0.28",
    "rollup": "0.49.3",
    "rxjs": "5.0.0-beta.12",
    "tslint": "^3.15.1",
    "typescript": "2.9.2",
    "typings": "2.1.1",
    "zone.js": "0.6.23"
